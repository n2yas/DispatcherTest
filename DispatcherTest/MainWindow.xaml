<Window x:Class="DispatcherTest.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DispatcherTest"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel>
        <Button x:Name="btnCounter" Click="btnCounter_Click">Counter</Button>
        <TextBox x:Name="txtUri"/>
        <Button x:Name="btnDownloadImage" Click="btnDownloadImage_Click">Download Image</Button>
        <Image x:Name="imgMain"/>
    </StackPanel>
</Window>
